Configuration perfo.conf {
	target Capella
	project org.polarsys.capella.vp.perfo
	nsuri "http://www.polarsys.org/capella/perfo"
	release {
		version : 1.2.0.qualifier
		description : "Capella Basic Performance Viewpoint"
		execution environments : "JavaSE-1.7"
	}
	generation {
		data (
			Model: true
			Edit: true
			Editor: false
			Test: false
			Javadoc: false
			OverwriteEcore: true
		)
		diagram (
			OverwriteOdesign: true
		)
		documentation (
			EcoreToHtml: false
		)
	}
}